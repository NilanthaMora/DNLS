@model IEnumerable<IdentityExtension.Models.Equipment>

@{
    ViewData["Title"] = "EquipmentG47Details";
    ViewData["Nav"] = "nav";
}

<div class="container">
    <div class="card-group">
        <div class="row">
            <div class="card col-md-2 equip-col"></div>
            <div class="card col-md-8 equip-col"><h2 class="head_title">@ViewBag.menu G47 Details</h2></div>
            <div class="card col-md-2 equip-col back-btn">
                <a asp-controller="EquipmentViewManagement" asp-action="ViewIndexMenu" asp-route-id="g47">
                    <img src="~/images/back2.png" alt="..." class="img-thumbnail equip-back">
                </a>
            </div>
        </div>
    </div>
</div>
<hr />
<table class="equip_table_admin table table-striped">
    <thead>
        <tr>
            <th class="eq_th_admin">
                @Html.DisplayNameFor(model => model.srNo)
            </th>
            <th class="eq_th_admin">
                @Html.DisplayNameFor(model => model.make)
            </th>
            <th class="eq_th_admin">
                @Html.DisplayNameFor(model => model.model)
            </th>
            <th class="eq_th_admin">
                @Html.DisplayNameFor(model => model.Base)
            </th>
            <th class="eq_th_admin">
                @Html.DisplayNameFor(model => model.state)
            </th>
            <th class="eq_th_admin">
                @Html.DisplayNameFor(model => model.remarks)
            </th>
            <th class="eq_th_admin">
                @Html.DisplayNameFor(model => model.cDate)
            </th>
            <th class="eq_th_admin">
                @Html.DisplayNameFor(model => model.action)
            </th>
            <th class="eq_th_admin">
                @Html.DisplayNameFor(model => model.g47Remarks)
            </th>
            <th class="eq_th_admin">
                @Html.DisplayNameFor(model => model.g47Date)
            </th>
            <th class="eq_th_admin">
                @Html.DisplayNameFor(model => model.del)
            </th>
            <th class="eq_th_admin">
                @Html.DisplayNameFor(model => model.delDate)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td class="eq_th_admin">
                @Html.DisplayFor(modelItem => item.srNo)
            </td>
            <td class="eq_th_admin">
                @Html.DisplayFor(modelItem => item.make)
            </td>
            <td class="eq_th_admin">
                @Html.DisplayFor(modelItem => item.model)
            </td>
            <td class="eq_th_admin">
                @Html.DisplayFor(modelItem => item.Base.description)
            </td>
            <td class="eq_th_admin">
                @Html.DisplayFor(modelItem => item.state)
            </td>
            <td class="eq_th_admin">
                @Html.DisplayFor(modelItem => item.remarks)
            </td>
            <td class="eq_th_admin">
                @Html.DisplayFor(modelItem => item.cDate)
            </td>
            <td class="eq_th_admin">
                @Html.DisplayFor(modelItem => item.action)
            </td>
            <td class="eq_th_admin">
                @Html.DisplayFor(modelItem => item.g47Remarks)
            </td>
            <td class="eq_th_admin">
                @Html.DisplayFor(modelItem => item.g47Date)
            </td>
            <td class="eq_th_admin">
                @Html.DisplayFor(modelItem => item.del)
            </td>
            <td class="eq_th_admin">
                @Html.DisplayFor(modelItem => item.delDate)
            </td>
        </tr>
}
    </tbody>
</table>
