                        <label asp-for="trawlerRange" class="control-label"></label>
                        <input asp-for="trawlerRange" class="form-control" />
                        <span asp-validation-for="trawlerRange" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="ipcRange" class="control-label"></label>
                        <input asp-for="ipcRange" class="form-control" />
                        <span asp-validation-for="ipcRange" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="facRange" class="control-label"></label>
                        <input asp-for="facRange" class="form-control" />
                        <span asp-validation-for="facRange" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="fgbRange" class="control-label"></label>
                        <input asp-for="fgbRange" class="form-control" />
                        <span asp-validation-for="fgbRange" class="text-danger"></span>
                    </div>
