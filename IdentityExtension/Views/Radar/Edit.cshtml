                        <input asp-for="scannerLength" class="form-control" />
                        <span asp-validation-for="scannerLength" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="supply" class="control-label"></label>
                        <input asp-for="supply" class="form-control" />
                        <span asp-validation-for="supply" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="dateInstalled" class="control-label"></label>
                        <input asp-for="dateInstalled" class="form-control" />
